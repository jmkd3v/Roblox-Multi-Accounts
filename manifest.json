{
	"name": "Roblox Multi Accounts",
	"version": "1.0.0",
	"description": "Allows swapping between multiple Roblox accounts.",
	"manifest_version": 3,
	"permissions": [
		"declarativeContent",
		"cookies",
		"storage"
	],
	"content_scripts": [{
	  "js": ["scripts/inject.js"],
	  "css": ["styles/style.css"],
	  "matches": ["*://*.roblox.com/*"]
	}],
	"web_accessible_resources": [
	  {
		"resources": ["scripts/script.js", "styles/style.css"],
		"matches": [ "*://*.roblox.com/*" ]
	  }
	],
	"host_permissions": [
		"*://*.roblox.com/"
	]
}